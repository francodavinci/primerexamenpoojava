package primerparcial;

import java.util.ArrayList;

public class Cine {
	//atributos
	private String nombre;
	private String ciudad;
	private ArrayList<Pelicula> peliculas;
	
	//constructores
	public Cine(String nombre, String ciudad) {
		super();
		this.nombre = nombre;
		this.ciudad = ciudad;
	}


	public Cine(String nombre) {
		super();
		this.nombre = nombre;
	}
	
	
	
	
	
	
}
